{
  "name": "neuralhack-cognitive-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "tsc -p tsconfig.build.json && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:accessibility": "playwright test accessibility.spec.ts",
    "ionic:build": "ionic build",
    "ionic:serve": "ionic serve",
    "cap:add": "npx cap add",
    "cap:copy": "npx cap copy",
    "cap:sync": "npx cap sync",
    "cap:open": "npx cap open",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "migrate": "node scripts/migrate.js",
    "migrate:rollback": "node scripts/migrate.js rollback",
    "deploy": "npm run build && npx netlify deploy --prod --dir=dist",
    "deploy:preview": "npm run build && npx netlify deploy --dir=dist",
    "security:audit": "npm audit --audit-level=moderate"
  },
  "dependencies": {
    "@capacitor/cli": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@ionic/react": "^8.7.2",
    "@ionic/react-router": "^8.7.2",
    "@supabase/supabase-js": "^2.54.0",
    "@types/react-router-dom": "^5.3.3",
    "chart.js": "^4.5.0",
    "crypto-js": "^4.2.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "i18next": "^25.3.4",
    "jspdf": "^3.0.1",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.6.1",
    "react-router-dom": "^5.3.4",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@axe-core/react": "^4.10.2",
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.49.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/crypto-js": "^4.2.2",
    "@types/file-saver": "^2.0.7",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "axe-core": "^4.10.2",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jest-axe": "^8.0.0",
    "jsdom": "^26.1.0",
    "msw": "^2.8.4",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.1",
    "vitest": "^3.2.4"
  }
}